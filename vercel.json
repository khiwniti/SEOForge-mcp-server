{
  "version": 2,
  "name": "seoforge-mcp-server",
  "builds": [
    {
      "src": "backend-express/dist/server.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend-express/dist/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PORT": "3000",
    "HOST": "0.0.0.0",
    "LOG_LEVEL": "info",
    "CORS_ORIGINS": "*",
    "CORS_CREDENTIALS": "true",
    "JWT_SECRET": "your-jwt-secret-here",
    "VALID_API_KEYS": "test-api-key",
    "OPENAI_API_KEY": "",
    "GOOGLE_API_KEY": "",
    "ANTHROPIC_API_KEY": "",
    "REPLICATE_API_TOKEN": "",
    "TOGETHER_API_KEY": "",
    "ALLOW_REGISTRATION": "true",
    "ENABLE_SWAGGER_DOCS": "true",
    "ENABLE_METRICS": "true"
  },
  "functions": {
    "backend-express/dist/server.js": {
      "maxDuration": 30
    }
  },
  "buildEnvironment": {
    "NODE_ENV": "production"
  },
  "regions": ["iad1"],
  "framework": null
}